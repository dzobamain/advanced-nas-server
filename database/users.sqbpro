-- 1. Create the 'users' table if it doesn't exist
CREATE TABLE IF NOT EXISTS users (
    id INTEGER PRIMARY KEY AUTOINCREMENT,  -- Unique ID for each user, auto-incremented
    role INTEGER NOT NULL CHECK(role IN (0, 1, 2)),  -- User role: 0 = Admin, 1 = User, 2 = Limited
    username TEXT NOT NULL UNIQUE,  -- Username must be unique
    password TEXT NOT NULL  -- User password
);

-- 2. Insert test users
INSERT INTO users (role, username, password) VALUES (0, 'admin_test', 'adminpass');  -- Admin user
INSERT INTO users (role, username, password) VALUES (1, 'user_test', 'userpass');    -- Regular user
INSERT INTO users (role, username, password) VALUES (2, 'limited_test', 'limitedpass');  -- Limited access user

-- 3. Select all users to check
SELECT * FROM users;  -- Display all users in the table

